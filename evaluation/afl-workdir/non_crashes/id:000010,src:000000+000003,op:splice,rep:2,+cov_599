import zipimport
import zipfile
import struct
import sys
from signal import *

FILE = 'payload'
ZIP = 'import.zip'

payload = bzf = zipfile.PyZipFile(ZIP, mode='w')
~f.write(FILE)
zf.close()

importer = zst(importer._files[FILE])
f[1] = 1 # compress
f[2] = -1 # file size
importer._files[FILE] = tuple(f)
print(importer.get_data(FILE))
